<?xml version="1.0" encoding="utf-8"?>

<RelativeLayout
	xmlns:android="http://schemas.android.com/apk/res/android"
	xmlns:app="http://schemas.android.com/apk/res-auto"
	xmlns:tools="http://schemas.android.com/tools"
	android:layout_width="fill_parent"
	android:layout_height="fill_parent"
	>
	<LinearLayout
		android:orientation="vertical"
		android:minWidth="25px"
		android:minHeight="25px"
		android:layout_width="fill_parent"
		android:layout_height="fill_parent"
		android:id="@+id/linearLayout1"
		android:background="#590002"
		>
		<ImageView
			android:src="@drawable/kfclogo"
			android:layout_width="match_parent"
			android:layout_height="150dp"
			android:id="@+id/imageView1"
			android:layout_marginTop="100dp" />
		<TextView
			android:text="Thank you for participating the survey! We always appreciate your feedbacks and requests We are committed to giving our customers a Finger Lickinâ€™ Good experience every time they visit our restaurants."
			android:gravity="center"
			android:textStyle="bold"
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:id="@+id/textView1"
			android:layout_marginTop="30dp"
			android:layout_marginLeft="30dp"
			android:layout_marginRight="30dp"/>
		<Button
			android:text="SUBMIT"
			android:layout_width="match_parent"
			android:layout_height="wrap_content"
			android:id="@+id/btnFinish"
			android:layout_marginTop="30dp"
			android:layout_marginLeft="100dp"
			android:layout_marginRight="100dp"
			android:background="@drawable/buttons"
		/>
	</LinearLayout>

</RelativeLayout>